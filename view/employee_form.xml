<data>
    <xpath expr="/form/field[@name='end_date']" position="after">
        <notebook colspan="4">
            <page string="Personal info" col="4" id="personal">
                <group id="person_details" colspan="4">
                    <group id="person_name" colspan="2">
                        <label name="first_name"/>
                        <field name="first_name"/>
                        <label name="middle_name"/>
                        <field name="middle_name"/>
                        <label name="last_name"/>
                        <field name="last_name"/>
                        <label name="genre"/>
                        <field name="genre"/>
                        <label name="birth_date"/>
                        <field name="birth_date"/>
                        <label name="age"/>
                        <field name="age"/>
                        <label name="marital_status"/>
                        <field name="marital_status"/>
                        <label name="instruction_level"/>
                        <field name="instruction_level"/>
                        <label name="birth_country"/>
                        <field name="birth_country"/>
                        <label name="nationality"/>
                        <field name="nationality"/>
                        <label name="residence"/>
                        <field name="residence"/>
                    </group>
                    <group id="person_picture"  colspan="2">
                        <field xfill="0" xexpand="1" name="photo" 
                                    height="200" width="200" widget="image"/>
                    </group>
                </group>
                <newline/>
                <field name="dependents" colspan="4"/>
            </page>
            <page string="Job Info" col="4" id="job">
                <label name="position"/>
                <field name="position"/>
                <label name="department"/>
                <field name="department"/>
                <label name="supervisor"/>
                <field name="supervisor"/>
                <label name="contract_type"/>
                <field name="contract_type"/>
                <label name="payment_type"/>
                <field name="payment_type"/>
                <newline/>
                <field name="subordinates" colspan="4"/>
            </page>
            <page string="Documents" col="4" id="documents">
                <field name="documents" colspan="4"/>
            </page>
            <page string="Qualification" col="4" id="qualification">
                <field name="qualifications" colspan="4"/>
            </page>
        </notebook>
    </xpath>
</data>

